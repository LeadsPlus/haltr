<div id="invoice_wrapper">
  <div id="workspace1" class="haltrinvoice">
    <div id="workspace2">
      <div id="col1">

      <%= render :partial=>'invoices/invoice_original' %>

      <%= render :partial=>'invoices/events'%>

      <% if User.current.admin? %>
        <%= render :partial=>'invoices/channel_links'%>
      <% end %>

      </div>
    </div>
  </div>
</div>

<textarea id="console" rows="10" style="width:99%; visibility:hidden;">
</textarea>

<% unless @is_pdf -%>
  <% content_for :sidebar do %>
    <%= render_menu :invoices_menu %>
  <% end %>
  <% content_for :header_tags do %>
    <%= stylesheet_link_tag 'minimal', :plugin => 'haltr' %>
  <% end %>
<% end %>
